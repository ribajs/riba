{
  "name": "@ribajs/iconset",
  "description": "Iconset for Riba.js",
  "version": "2.0.0-alpha.0",
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "contributors": [],
  "url": "https://github.com/ribajs/riba/tree/master/packages/iconset",
  "homepage": "https://ribajs.com/",
  "main": "dist/svg.json",
  "module": "dist/svg.json",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ribajs/riba.git"
  },
  "keywords": [
    "iconset",
    "icon",
    "icons",
    "Riba",
    "Rivets",
    "tinybind",
    "SPA",
    "TypeScript",
    "Browser"
  ],
  "scripts": {
    "build": "npm run build:svg && npm run build:filelist:svg",
    "build:svg": "npm run clean:svg && gulp build:svg",
    "build:filelist": "gulp build:filelist",
    "build:filelist:svg": "gulp build:filelist:svg",
    "build:filelist:png": "gulp build:filelist:png",
    "build:png": "gulp build:png",
    "build:png:8": "gulp build:png:8",
    "build:png:16": "gulp build:png:16",
    "build:png:32": "gulp build:png:32",
    "build:png:64": "gulp build:png:64",
    "build:png:128": "gulp build:png:128",
    "build:png:256": "gulp build:png:256",
    "clean:svg": "gulp clean:svg",
    "clean": "echo 'Do not clean the iconset by default because we want to hold the build image files in the repo'"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.17.2",
    "core-js": "^3.21.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-debug": "^4.0.0",
    "gulp-exec": "^5.0.0",
    "gulp-filelist": "^2.0.5",
    "gulp-svgmin": "^4.1.0"
  },
  "bugs": {
    "url": "https://github.com/ribajs/riba/issues"
  }
