{
  "name": "@ribajs/empty-template",
  "description": "Empty Riba.js module, can be used as template for new modules",
  "version": "2.0.0-rc.20",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx,.cts,.mts --fix && tsc --noEmit",
    "build": "yarn run lint && yarn run build:module",
    "build:module": "tsc",
    "clean": "rm -rf ./dist ./lib"
  },
  "dependencies": {
    "@ribajs/core": "workspace:^",
    "@ribajs/jsx": "workspace:^",
    "@ribajs/utils": "workspace:^"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "source": "src/index.ts",
  "devDependencies": {
    "@babel/runtime": "^7.26.0",
    "@babel/runtime-corejs3": "^7.26.0",
    "@ribajs/tsconfig": "workspace:^",
    "@types/eslint": "^8.56.12",
    "@types/node": "^20.17.6",
    "eslint": "^8.57.1",
    "typescript": "5.6.3"
  }
}
