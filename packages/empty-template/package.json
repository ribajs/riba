{
  "name": "@ribajs/empty-template",
  "description": "Empty Riba.js module, can be used as template for new modules",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx,.cts,.mts --fix && tsc --noEmit",
    "build": "npm run lint && npm run build:module",
    "build:module": "tsc",
    "clean": "rm -rf ./dist ./lib"
  },
  "dependencies": {
    "@ribajs/core": "workspace:^",
    "@ribajs/jsx": "workspace:^",
    "@ribajs/utils": "workspace:^"
  },
  "main": "./src/index.ts",
  "module": "src/index.ts",
  "source": "src/index.ts",
  "devDependencies": {
    "@babel/runtime": "^7.17.8",
    "@babel/runtime-corejs3": "^7.17.8",
    "@ribajs/tsconfig": "workspace:^",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.22",
    "eslint": "^8.11.0",
    "typescript": "^4.6.2"
  }
}
