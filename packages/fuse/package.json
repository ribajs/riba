{
  "name": "@ribajs/fuse",
  "description": "Riba.js module with utilities based on Fuse.js",
  "version": "2.0.0-rc.14",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx,.cts,.mts --fix && tsc --noEmit",
    "build": "yarn run lint && yarn run build:module",
    "build:module": "tsc",
    "clean": "rm -rf ./dist ./lib"
  },
  "dependencies": {
    "@ribajs/core": "workspace:^",
    "@ribajs/jsx": "workspace:^",
    "@ribajs/utils": "workspace:^",
    "fuse.js": "^7.0.0"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "source": "src/index.ts",
  "devDependencies": {
    "@babel/runtime": "^7.23.9",
    "@babel/runtime-corejs3": "^7.23.9",
    "@ribajs/tsconfig": "workspace:^",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "typescript": "5.3.3"
  }
}
