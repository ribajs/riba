  "homepage": "https://ribajs.com/",
  "bin": {
    "riba": "dist/bin/riba.js"
  },
  "scripts": {
    "build": "(npm run clean) && (npm run build:ts) && (chmod +x ./dist/bin/riba.js)",
    "build:ts": "npm run lint && tsc",
    "clean": "rm -rf ./dist",
    "watch": "npm run build:ts -- --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "packages:npm:link": "npm link @ribajs/core @ribajs/schematics",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
