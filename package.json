{
  "name": "ribajs",
  "description": "Declarative data binding + template solution.",
  "version": "2.0.0-rc.18",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "private": true,
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "contributors": [],
  "main": "src/index.ts",
  "url": "https://github.com/ribajs/riba",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/riba.git"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Bootstrap5",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "SPA",
    "TypeScript",
    "Browser"
  ],
  "scripts": {
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install && yarn dlx @yarnpkg/sdks vscode",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:clear": "jest --config=jest.config.cjs --clearCache",
    "test:migrate": "ts-jest config:migrate jest.config.cjs",
    "lint": "yarn run lint:ext && yarn workspaces foreach -v -p -W run lint",
    "lint:ext": "node lint-file-extension.cjs",
    "build:all": "yarn workspaces foreach -v -W --jobs 5 run build",
    "build:libs": "yarn workspaces foreach -v -p -W --no-private --jobs 5 run build",
    "clean": "yarn workspaces foreach -v -p -W run clean",
    "bump:prerelease": "yarn workspaces foreach -v version prerelease",
    "bump:patch": "yarn workspaces foreach -v version patch",
    "bump:minor": "yarn workspaces foreach -v version minor",
    "bump:major": "yarn workspaces foreach -v version major",
    "publish:next": "yarn workspaces foreach -v -W --no-private npm publish --tolerate-republish --tag next --access public",
    "publish:latest": "yarn workspaces foreach -v -W --no-private npm publish --tolerate-republish --tag latest --access public"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-export-default-from": "^7.24.6",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/plugin-transform-typescript": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@babel/runtime-corejs3": "^7.24.6",
    "@babel/types": "^7.24.6",
    "@jest/globals": "^29.7.0",
    "@ribajs/babel-config": "workspace:^",
    "@ribajs/eslint-config": "workspace:^",
    "@ribajs/tsconfig": "workspace:^",
    "@ribajs/types": "workspace:^",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/core-js": "^2.5.8",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@yarnpkg/pnpify": "^4.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-array-includes": "^2.0.3",
    "core-js": "^3.37.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "pkg-dir": "^8.0.0",
    "prettier": "^3.3.0",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.77.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.4",
    "typescript": "5.4.5",
    "webpack-glob-entry": "^2.1.1"
  },
  "workspaces": [
    "packages/*",
    "vendor/*",
    "infra/*",
    "!infra/cli",
    "infra/doc",
    "!infra/schematics",
    "backend/*",
    "demos/*",
    "examples/*/*",
    "!examples/electron/electron",
    "!examples/electron/electron-nest"
  ],
  "dependenciesMeta": {
    "prettier": {
      "unplugged": true
    }
  },
  "resolutions": {
    "@ribajs/bs4": "workspace:^",
    "@ribajs/bs5": "workspace:^",
    "@ribajs/content-slider": "workspace:^",
    "@ribajs/octobercms": "workspace:^",
    "@ribajs/leaflet-map": "workspace:^",
    "@ribajs/cache": "workspace:^",
    "@ribajs/core": "workspace:^",
    "@ribajs/extras": "workspace:^",
    "@ribajs/events": "workspace:^",
    "@ribajs/history": "workspace:^",
    "@ribajs/i18n": "workspace:^",
    "@ribajs/iconset": "workspace:^",
    "@ribajs/jquery": "workspace:^",
    "@ribajs/moment": "workspace:^",
    "@ribajs/photoswipe": "workspace:^",
    "@ribajs/router": "workspace:^",
    "@ribajs/shopify": "workspace:^",
    "@ribajs/shopify-easdk": "workspace:^",
    "@ribajs/shopify-tda": "workspace:^",
    "@ribajs/utils": "workspace:^",
    "@ribajs/doc": "workspace:^",
    "@ribajs/eslint-config": "workspace:^",
    "@ribajs/types": "workspace:^",
    "@ribajs/webpack-config": "workspace:^",
    "@ribajs/tsconfig": "workspace:^",
    "@ribajs/nest-theme": "workspace:^",
    "@ribajs/npm-package": "workspace:^",
    "@ribajs/babel-config": "workspace:^",
    "@ribajs/strapi": "workspace:^"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "glob": "^10.4.1",
    "pug": "^3.0.3"
  },
  "packageManager": "yarn@4.2.2"
}
