{
  "name": "ribajs",
  "description": "Declarative data binding + template solution.",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "private": true,
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "contributors": [],
  "main": "./src/index.ts",
  "url": "https://github.com/ribajs/riba",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/riba/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/riba.git"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Bootstrap5",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "SPA",
    "TypeScript",
    "Browser"
  ],
  "scripts": {
    "workspace-foreach": "yarn node workflow/workspace-foreach.js",
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:clear": "jest --config=jest.config.js --clearCache",
    "test:migrate": "ts-jest config:migrate jest.config.js",
    "lint": "yarn workspaces foreach run lint",
    "build": "yarn workspaces foreach run build",
    "clean": "yarn workspaces foreach run clean",
    "bump:prerelease": "yarn workspaces foreach version prerelease",
    "bump:patch": "yarn workspaces foreach version patch",
    "bump:minor": "yarn workspaces foreach version minor",
    "bump:major": "yarn workspaces foreach version major",
    "publish:next": "yarn run build && yarn workspaces foreach npm publish --tolerate-republish --tag next --access public",
    "publish:latest": "yarn run build && yarn workspaces foreach npm publish --tolerate-republish --tag latest --access public"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-export-default-from": "^7.16.7",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@babel/runtime-corejs3": "^7.17.2",
    "@babel/types": "^7.17.0",
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/testing": "^8.4.0",
    "@ribajs/babel-config": "workspace:^",
    "@ribajs/eslint-config": "workspace:^",
    "@ribajs/tsconfig": "workspace:^",
    "@ribajs/types": "workspace:^",
    "@types/core-js": "^2.5.5",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/experimental-utils": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@yarnpkg/pnpify": "^3.1.1",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-array-includes": "^2.0.3",
    "core-js": "^3.21.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "jest-extended": "^2.0.0",
    "pkg-dir": "^6.0.1",
    "prettier": "2.5.1",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.49.9",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.3",
    "typescript": "4.6.2",
    "webpack-glob-entry": "^2.1.1"
  },
  "workspaces": [
    "packages/*",
    "vendor/*",
    "infra/*",
    "!infra/cli",
    "infra/doc",
    "!infra/schematics",
    "backend/*",
    "demos/*",
    "!demos/vue",
    "examples/*/*",
    "!examples/electron/electron",
    "!examples/electron/electron-nest"
  ],
  "dependenciesMeta": {
    "prettier": {
      "unplugged": true
    }
  },
  "resolutions": {
    "@ribajs/bs4": "workspace:^",
    "@ribajs/bs5": "workspace:^",
    "@ribajs/content-slider": "workspace:^",
    "@ribajs/octobercms": "workspace:^",
    "@ribajs/leaflet-map": "workspace:^",
    "@ribajs/cache": "workspace:^",
    "@ribajs/core": "workspace:^",
    "@ribajs/extras": "workspace:^",
    "@ribajs/events": "workspace:^",
    "@ribajs/history": "workspace:^",
    "@ribajs/i18n": "workspace:^",
    "@ribajs/iconset": "workspace:^",
    "@ribajs/jquery": "workspace:^",
    "@ribajs/moment": "workspace:^",
    "@ribajs/photoswipe": "workspace:^",
    "@ribajs/router": "workspace:^",
    "@ribajs/shopify": "workspace:^",
    "@ribajs/shopify-easdk": "workspace:^",
    "@ribajs/shopify-tda": "workspace:^",
    "@ribajs/utils": "workspace:^",
    "@ribajs/doc": "workspace:^",
    "@ribajs/eslint-config": "workspace:^",
    "@ribajs/types": "workspace:^",
    "@ribajs/webpack-config": "workspace:^",
    "@ribajs/tsconfig": "workspace:^",
    "@ribajs/nest-theme": "workspace:^",
    "@ribajs/npm-package": "workspace:^",
    "@ribajs/babel-config": "workspace:^",
    "@ribajs/strapi": "workspace:^"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "glob": "^7.2.0",
    "pug": "^3.0.2"
  },
  "packageManager": "yarn@3.2.0"
}
