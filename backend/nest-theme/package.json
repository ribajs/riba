{
  "name": "@ribajs/nest-theme",
  "version": "2.0.0-alpha.0",
  "description": "Nest.js module to build Riba.js applications",
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "scripts": {
    "clear": "rimraf dist",
    "build": "npm run lint && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:types": "tsc --project ./tsconfig.json --declaration true --emitDeclarationOnly --outDir dist/types",
    "build:esm": "tsc --project ./tsconfig.json --declaration false --module esnext --outDir dist/esm",
    "build:cjs": "tsc --project ./tsconfig.json --declaration false --module commonjs --outDir dist/cjs",
    "watch": "npm run build -- --watch",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@nestjs/common": "^8.0.10",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.10",
    "@nestjs/platform-express": "^8.0.10",
    "@ribajs/core": "workspace:*",
    "@ribajs/events": "workspace:*",
    "@ribajs/ssr": "workspace:*",
    "@types/consolidate": "^0.14.1",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.13",
    "cache-manager": "^3.4.4",
    "cheerio": "^1.0.0-rc.10",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsdom": "^17.0.0",
    "node-fetch": "^2.6.4",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "whatwg-encoding": "^2.0.0",
    "yaml": "next"
  },
  "peerDependencies": {
    "pug": "^3.0.0",
    "tinyliquid": "^0.2.34",
    "twig": "^1.15.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.10",
    "@types/cache-manager": "^3.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/node-fetch": "^3.0.3",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
