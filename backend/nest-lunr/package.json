{
  "name": "@ribajs/nest-lunr",
  "version": "2.0.0-alpha.0",
  "description": "Nest.js module to use Lunr.js in your in your Nest.js application.",
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "clear": "rimraf dist",
    "build": "npm run lint && npm run typecheck && npm run clear && tsc -p tsconfig.json",
    "watch": "npm run build -- --watch",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@types/express": "^4.17.13",
    "@types/lunr": "^2.3.4",
    "lunr": "^2.3.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6"
  },
  "peerDependencies": {
    "pug": "^3.0.0",
    "tinyliquid": "^0.2.34",
    "twig": "^1.15.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
